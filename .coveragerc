[run]
branch = True
source =
    nginx_proxy
    nginx
    cloudflare
omit =
    tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */site-packages/*
    setup.py
    */conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abstract
precision = 2
show_missing = True

[html]
directory = htmlcov

[xml]
output = coverage.xml
